{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
      },
    "rules": {
        "comma-dangle": 0,
        "max-len": [
            "error",
            { "code": 80 }
          ],
          "import/no-unresolved": [
              2,
              {
                "commonjs": true
              }
            ]
    }
}
