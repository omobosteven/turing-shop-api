{
  "name": "turing-shop-api",
  "version": "1.0.0",
  "description": "Turing shop is an e-commerce system which allows users to to easily do their shopping without hassle.",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "build": "npm run clean:build && babel src -d dist --ignore node_modules/",
    "clean:build": "rimraf dist/",
    "start-dev": "nodemon src/index.js --exec babel-node",
    "test": "NODE_ENV=test nyc --reporter=html mocha --require @babel/register src/test/**/*.spec.js --exit && nyc report --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omobosteven/turing-shop.git"
  },
  "author": "stephen omobo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omobosteven/turing-shop/issues"
  },
  "homepage": "https://github.com/omobosteven/turing-shop#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.5",
    "rimraf": "^2.6.3",
    "sequelize": "^5.3.5",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "nyc": "^13.3.0"
  }
}
